<p id="notice"><%= notice %></p>

<h1>Listing Prerequisits</h1>

<table>
  <thead>
    <tr>
      <th>Skill</th>
      <th>Prerequisit skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prerequisits.each do |prerequisit| %>
      <tr>
        <td><%= prerequisit.skill_id %></td>
        <td><%= prerequisit.prerequisit_skill_id %></td>
        <td><%= link_to 'Show', prerequisit %></td>
        <td><%= link_to 'Edit', edit_prerequisit_path(prerequisit) %></td>
        <td><%= link_to 'Destroy', prerequisit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prerequisit', new_prerequisit_path %>
