<p id="notice"><%= notice %></p>

<h1>Listing Demanded Skills</h1>

<table>
  <thead>
    <tr>
      <th>Employer user</th>
      <th>Skill</th>
      <th>Level</th>
      <th>Created at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demanded_skills.each do |demanded_skill| %>
      <tr>
        <td><%= demanded_skill.employer_user_id %></td>
        <td><%= demanded_skill.skill_id %></td>
        <td><%= demanded_skill.level %></td>
        <td><%= demanded_skill.created_at %></td>
        <td><%= link_to 'Show', demanded_skill %></td>
        <td><%= link_to 'Edit', edit_demanded_skill_path(demanded_skill) %></td>
        <td><%= link_to 'Destroy', demanded_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demanded skill', new_demanded_skill_path %>
